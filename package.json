{
  "name": "jupyter-display-area",
  "version": "0.0.0",
  "description": "Prototype Display area for Jupyter kernel rich output",
  "main": "dist/jupyter-display-area.html",
  "readme": "README.md",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build:js && npm run build:html && npm run build:vulcanize",
    "build:js": "mkdir -p dist && browserify src/*.js -t babelify --outfile dist/jupyter-display-area.js",
    "build:html": "mkdir -p dist && cp src/jupyter-display-area.html dist/jupyter-display-area.local.html",
    "build:vulcanize": "mkdir -p dist && vulcanize --inline-scripts dist/jupyter-display-area.local.html > dist/jupyter-display-area.html",
    "clean": "rm -r dist/*"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rgbkrk/jupyter-display-area.git"
  },
  "author": "Kyle Kelley",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/rgbkrk/jupyter-display-area/issues"
  },
  "dependencies": {
    "babelify": "^6.1.2",
    "browserify": "^10.2.6",
    "vulcanize": "^1.10.1"
  }
}
