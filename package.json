{
  "name": "jupyter-display-area",
  "version": "0.0.0",
  "description": "Prototype Display area for Jupyter kernel rich output",
  "main": "dist/jupyter-display-area.html",
  "readme": "README.md",
  "scripts": {
    "postinstall": "npm run build",
    "test": "echo 'This is totally not a test.'",
    "prebuild": "mkdirp dist && npm run clean",
    "build": "npm run build:js && npm run build:html && npm run build:vulcanize",
    "build:js": "browserify src/*.js -t babelify --outfile dist/jupyter-display-area.js",
    "build:html": "cp src/jupyter-display-area.html dist/jupyter-display-area.local.html",
    "build:vulcanize": "vulcanize --inline-scripts dist/jupyter-display-area.local.html > dist/jupyter-display-area.html",
    "watch:build": "nodemon -q -w src/ --ext '.' --exec 'npm run build'",
    "clean": "rimraf dist/*"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rgbkrk/jupyter-display-area.git"
  },
  "author": "Kyle Kelley",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/rgbkrk/jupyter-display-area/issues"
  },
  "dependencies": {
    "babelify": "^6.1.2",
    "browserify": "^10.2.6",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.3.7",
    "rimraf": "^2.4.1",
    "vulcanize": "^1.10.1"
  }
}
